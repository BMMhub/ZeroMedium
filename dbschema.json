{
    "db_name": "ZeroNetDevCenter",
    "db_file": "data/zeromedium.db",
    "version": 2,
    "maps": {
        "tutorials.json": {
            "to_table": [
                {
                    "node": "tutorials",
                    "table": "tutorials"
                }
            ]
        },
        "blog.json": {
            "to_table": ["blogposts"]
        },
        "tags.json": {
            "to_table": ["tags"]
        },
        "users/.+/data.json": {
            "to_keyvalue": [ "name", "about", "interests" ]
        },
        "users/.+/content.json": {
            "to_json_table": ["cert_user_id"],
            "file_name": "data.json"
        }
    },
    "tables": {
        "json": {
            "cols": [
                ["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
                ["directory", "TEXT"],
                ["file_name", "TEXT"],
                ["cert_user_id", "TEXT"]
            ],
            "indexes": ["CREATE UNIQUE INDEX path ON json(directory, file_name)"],
            "schema_changed": 12
        }
    }
}
